{
  "private": false,
  "name": "investigator-fvtt",
  "version": "9.15",
  "type": "module",
  "description": "Official GUMSHOE support for FoundryVTT. Ships with compatiblity for several settings, and can be customized to work with any other setting.",
  "scripts": {
    "build": "vite build",
    "build:dev": "vite build --mode dev",
    "build:watch": "vite build --watch --mode dev",
    "link": "./tasks.ts link",
    "dev": "vite dev  & pnpm run typecheck:watch",
    "start": "pnpm dev",
    "clean": "./tasks.ts clean",
    "build-pack-translations": "./tasks.ts buildPackTranslations",
    "pull-translations": "tx pull --workers 16 --force -all",
    "test-types": "vitest --typecheck.only",
    "test": "vitest",
    "format:check": "prettier --check src",
    "format:fix": "prettier --write src",
    "lint:check": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --build --force",
    "typecheck:watch": "tsc --build --watch",
    "env": "env",
    "pwd": "pwd",
    "do-prerelease": "./packages/shared-fvtt-bits/scripts/do-prerelease.sh",
    "do-full-release": "./packages/shared-fvtt-bits/scripts/do-full-release.sh",
    "update-legacy-release-branch": "./scripts/update-legacy-release-branch.sh",
    "extract-packs": "./tasks.ts extractPacks",
    "check": "pnpm run typecheck && pnpm run test --run && pnpm run lint:check && pnpm run format:check && pnpm run build"
  },
  "author": "",
  "license": "",
  "dependencies": {
